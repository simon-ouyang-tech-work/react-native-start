{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react",
    "lib": ["es5", "es6", "dom"],
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5",
    "typeRoots": [
      "custom_typings"
    ],
    "paths": {
      "src/*": ["src/*"],
      "components/*": ["src/components/*"],
      "screens/*": ["src/screens/*"],
      "navigators/*": ["src/navigators/*"],
    }
  },
  "include": ["./src/**/*.tsx", "index.d.ts"],
  "exclude": ["dist", "build", "node_modules"]
}
